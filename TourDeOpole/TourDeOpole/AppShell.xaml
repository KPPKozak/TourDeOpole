<?xml version="1.0" encoding="UTF-8"?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms" 
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:view="clr-namespace:TourDeOpole.Views"
       Title="TourDeOpole"
       x:Class="TourDeOpole.AppShell">


    <!--Tworzy tabbar czyli dolne menu-->
    <TabBar>
        <ShellContent Title="Home" Icon="icon_home.svg" ContentTemplate="{DataTemplate view:PlaceView}" />
        <ShellContent Title="Wycieczki" Icon="icon_localization.svg" ContentTemplate="{DataTemplate view:TripView}" />
        <ShellContent Title="Ustawienia" Icon="icon_cog.svg" ContentTemplate="{DataTemplate view:SettingView}" />
    </TabBar>

</Shell>
